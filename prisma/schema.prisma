generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Anggota {
  id        Int           @id @default(autoincrement())
  nama      String
  fakultas  String
  jurusan   String
  angkatan  Int
  jenis     AnggotaType?
  divisi    Divisi?       @relation(fields: [divisiId], references: [id])
  divisiId  Int?
}

enum AnggotaType {
  PJ
  WA_PJ
  WA_PJ2
  STAFF
}

model Divisi {
  id       Int              @id @default(autoincrement())
  nama     String           @unique
  jenis    DivisiType
  anggota  Anggota[]
}

enum DivisiType {
  PENGURUS_INTI
  BPH
}